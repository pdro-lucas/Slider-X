<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('partials/head') %>
    <link href="/css/main.css" rel="stylesheet" />
    <%- title %>
  </head>
  <body id="body-pd">
    <%- include('components/Header') %> <%- include('components/Navbar/Navbar') %>

    <main>
      <div class="loader-container" id="particles-js">
        <div class="loader"></div>
        <div class="loader-text">
          <h1>SliderX</h1>
          <p>Loading...</p>
        </div>
      </div>
      <div class="container">
        <div class="dashboard">
          <h1 class="dashboard_title"><%= dashboard_title %></h1>
          <p class="dashboard_location">Dashboard > <%= dashboard_location %></p>
        </div>
        <%- include('components/'+component), {results} %>
      </div>
    </main>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
      crossorigin="anonymous">
    </script>
  </body>
</html>
