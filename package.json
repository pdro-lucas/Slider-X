  "description": "Dynamic Carousel",
  "main": "app.js",
  "author": "Pedro Lucas <pdrolucas.dev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
